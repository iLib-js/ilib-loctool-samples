{
    "name": "sample-webos-dart",
    "id": "sample-webos-dart",
    "projectType": "webos-dart",
    "sourceLocale": "en-KR",
    "pseudoLocale":  {
        "zxx-XX": "debug",
        "zxx-Hebr-XX": "debug-rtl",
        "zxx-Cyrl-XX": "debug-cyrillic",
        "zxx-Hans-XX": "debug-han-simplified",
        "as-XX" : "debug-font"
    },
    "resourceDirs": {
        "json":"assets/i18n"
    },
    "resourceFileTypes": {
        "json":"ilib-loctool-webos-json-resource"
    },
    "plugins": [
        "ilib-loctool-webos-dart"
    ],
    "excludes": [
        ".*",
        "test",
        "xliffs",
        "common"
    ],
    "settings": {
        "xliffsDir": "./xliffs",
        "locales":[
            "fr-CA",
            "fr-FR"
        ],
        "resourceFileNames": {
            "dart": "[locale].json",
            "c": "cstrings.json"
        },
        "jsonMap": {
            "mappings": {
                "**/appinfo.json": {
                    "template": "[dir]/[resourceDir]/[localeDir]/[filename]"
                },
                "**/qcardinfo.json": {
                    "template": "[dir]/[resourceDir]/[localeDir]/[filename]"
                },
                "[locale].json" : {
                    "template": "[dir]/[resourceDir]/[localeUnder].json"
                }
            }
        }
    }
}
