{
    "name": "sample-md",
    "id": "sample-md",
    "projectType": "custom",
    "sourceLocale": "en-US",
    "pseudoLocale": "zxx-XX",
    "resourceDirs": {
        "java":"res"
    },
    "plugins": [
        "ilib-loctool-ghfm"
    ],
    "excludes": [
        "node_modules",
        "xliffs",
        "locale"
    ],
    "settings": {
        "xliffsDir": "./xliffs",
        "locales":[
            "ko-KR",
            "nl-NL"
        ],
        "markdown": {
            "mappings": {
                "**/en-US/*.md": {
                    "template": "locale/[locale]/[filename]",
                    "frontmatter": ["Title", "Description"]
                }
            }
        }
    }
}
