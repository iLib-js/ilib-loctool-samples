{
    "name": "sample-webos-js",
    "id": "sample-webos-js",
    "projectType": "custom",
    "sourceLocale": "en-KR",
    "pseudoLocale":  {
        "zxx-XX": "debug",
        "zxx-Hebr-XX": "debug-rtl",
        "zxx-Cyrl-XX": "debug-cyrillic",
        "zxx-Hant-XX":"debug-asian",
        "as-XX" : "debug-font"
    },
    "resourceDirs": {
        "json":"resources"
    },
    "resourceFileTypes": {
        "json":"ilib-loctool-webos-json-resource"
    },
    "plugins": [
        "ilib-loctool-webos-javascript",
        "ilib-loctool-webos-appinfo-json"
    ],
    "excludes": [
        ".*",
        "test",
        "xliffs",
        "common"
    ],
    "settings": {
        "xliffsDir": "./xliffs",
        "locales":[
            "as-IN",
            "de-DE",
            "es-CO",
            "es-ES",
            "en-US",
            "fr-CA",
            "fr-FR",
            "ja-JP",
            "ko-KR",
            "ko-US",
            "en-AU",
            "en-GB",
            "en-AU",
            "ko-KR"
        ],
        "localeMap": {
            "fr-CA": "fr"
        },
        "localeInherit": {
            "en-CN": "en-GB",
            "en-AU": "en-GB"
        },
        "webos": {
            "commonXliff": "./common"
        }
    }
}
